{
  "name": "smol-app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "link": "ntl init",
    "dev": "astro dev",
    "start": "ntl dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy:hasura": "cd hasura && flyctl deploy smol-hasura",
    "graphql:generate": "gql-gen --config codegen.ts -r dotenv/config --watch",
    "docker-up": "docker compose --env-file .env up -d",
    "docker-down": "docker compose --env-file .env down",
    "go:watch": "PORT=4000 gow -e=go,mod,tmpl run ./go-server/main.go",
    "seed-apply": "hasura seed apply --project hasura --database-name default",
    "metadata-apply": "hasura metadata apply --project hasura",
    "metadata-reload": "hasura metadata reload --project hasura",
    "migrate-apply": "hasura migrate apply --project hasura --database-name default",
    "hasura:bootstrap": "pnpm migrate-apply && pnpm seed-apply && pnpm metadata-apply && hasura console --project hasura",
    "ci:metadata-reload": "hasura metadata reload --project hasura --endpoint $HASURA_URL  --admin-secret $HASURA_ADMIN_SECRET --skip-update-check",
    "ci:metadata-apply": "hasura metadata apply --project hasura --endpoint $HASURA_URL  --admin-secret $HASURA_ADMIN_SECRET --skip-update-check",
    "ci:migrate-apply": "hasura migrate apply --project hasura --endpoint $HASURA_URL  --admin-secret $HASURA_ADMIN_SECRET --database-name default --skip-update-check"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@astrojs/netlify": "^5.1.3",
    "@astrojs/react": "^3.1.0",
    "@astrojs/tailwind": "^5.1.0",
    "@floating-ui/react": "^0.25.1",
    "@headlessui/react": "^1.7.16",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "@netlify/functions": "^2.6.0",
    "@tabler/icons-react": "^2.30.0",
    "astro": "^4.5.6",
    "astro-icon": "^0.8.1",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^2.30.0",
    "discord.js": "^14.12.1",
    "fast-fuzzy": "^1.12.0",
    "graphql": "^16.7.1",
    "graphql-ws": "^5.14.0",
    "javascript-time-ago": "^2.5.9",
    "jsonwebtoken": "^9.0.1",
    "nanostores": "^0.9.3",
    "netlify-cli": "^17.19.5",
    "node-fetch": "^3.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-time-ago": "^7.2.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "tiny-invariant": "^1.3.1",
    "title": "^3.5.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@parcel/watcher": "^2.2.0",
    "@types/fuzzy-search": "^2.1.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/title": "^3.4.1",
    "dotenv": "^16.3.1",
    "hasura-cli": "^2.36.1"
  }
}
